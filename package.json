{
  "name": "voidguard-ai-governance-suite",
  "version": "1.0.0",
  "description": "Complete Enterprise AI Safety & Governance Platform",
  "main": "src/app.js",
  "private": true,
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "npm run build:docs",
    "build:docs": "swagger-jsdoc -d docs/swagger.config.js src/routes/*.js -o docs/openapi.json",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "deploy:dev": "node scripts/deploy-dev.js",
    "deploy:prod": "node scripts/deploy-prod.js"
  },
  "keywords": [
    "ai-governance",
    "ai-safety",
    "enterprise",
    "voidguard",
    "oracle",
    "voice-assistant",
    "algorithm-transparency",
    "consciousness-analysis"
  ],
  "author": "Ricardo Amaral (Brevvi) <team@silverbullet.live>",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "https://github.com/silverbullet-research/voidguard-ai-governance-suite"
  },
  "bugs": {
    "url": "https://github.com/silverbullet-research/voidguard-ai-governance-suite/issues"
  },
  "homepage": "https://voidguard.ai",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.3",
    "pg": "^8.11.1",
    "redis": "^4.6.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "eslint": "^8.44.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/app.js"
    ]
  }
}
